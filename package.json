{
  "name": "peoplesync_backend",
  "version": "0.0.1",
  "description": "This is backend for People Sync",
  "author": "https://github.com/shahadathhs",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "ci:check": "pnpm format && pnpm lint && pnpm db:validate && pnpm build && pnpm start",
    "ci:fix": "pnpm format:fix && pnpm lint:fix",
    "prisma": "npx prisma",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:reset": "npx prisma migrate reset",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:validate": "npx prisma validate",
    "db:format": "npx prisma format",
    "postinstall": "npx prisma generate",
    "prepare": "husky"
  },
