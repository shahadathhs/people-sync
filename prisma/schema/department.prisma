model Department {
  id   String @id @default(uuid())
  name String @unique

  employees User[]

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt

  @@map("departments")
}

model JobTitle {
  id   String @id @default(uuid())
  name String @unique

  employees User[]

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt

  @@map("job_titles")
}
